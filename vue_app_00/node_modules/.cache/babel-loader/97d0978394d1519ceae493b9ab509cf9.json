>a{\r\n    display: block;\r\n    background: #b0916a;\r\n    border-radius: 15px;\r\n    color: #fff;\r\n    text-decoration: none;\r\n    width: 90px;\r\n    line-height: 30px;\r\n    margin: 20px auto;\r\n    font-weight: normal;\r\n    font-size: 14px;\r\n}\r\n</style>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return {\r\n            img1:[],\r\n            img2:[],\r\n            img3:[],\r\n            uname:'',\r\n            phone:'',\r\n            upwd:'',\r\n            checkpwd:''\r\n        }\r\n    },\r\n    methods:{\r\n        open() {\r\n            this.$notify({\r\n                title: '成功',\r\n                message: '恭喜您注册成功！三秒后自动前往登陆界面',\r\n                type: 'success'\r\n            })\r\n        },\r\n        regGo(){\r\n            var uname=this.uname;\r\n            var phone=this.phone;\r\n            var upwd=this.upwd;\r\n            var checkpwd=this.checkpwd;\r\n            var reg1=/^[0-9a-zA-Z]{6,12}|[\\u4e00-\\u9fa5]{3,6}/;\r\n            var reg2=/^[0-9]{11}/;\r\n            var reg3=/^[0-9a-zA-Z]{8,16}/;\r\n            if(!reg1.test(uname)){\r\n                console.log(\"用户名不符合规则\");\r\n                return;\r\n            }\r\n            if(!reg2.test(phone)){\r\n                console.log(\"手机号不符合规则\");\r\n                return;\r\n            }\r\n            if(!reg3.test(upwd)){\r\n                console.log(\"密码不符合规则\");\r\n                return;\r\n            }\r\n            if(upwd!=checkpwd){\r\n                console.log(\"两次密码不一致\");\r\n                return;\r\n            }\r\n            var url=\"http://127.0.0.1:3000/users/reg\";\r\n            this.axios.post(url,`uname=${uname}&phone=${phone}&upwd=${upwd}`).then(res=>{\r\n                console.log(res);\r\n                if(res.data.code==1){\r\n                    this.open();\r\n                    setTimeout(()=>{\r\n                        this.$router.push(\"/login\")\r\n                    },3000)\r\n                }\r\n            })\r\n\r\n        },\r\n        regLoad(){\r\n            var url=\"http://127.0.0.1:3000/reg/bg\";\r\n            this.axios.get(url).then(result=>{\r\n                console.log(result.data);\r\n                this.img1=result.data.img1[0];\r\n                this.img2=result.data.img2[0];\r\n                this.img3=result.data.img2[1];\r\n            })\r\n        }\r\n    },\r\n    created() {\r\n        this.regLoad()\r\n    }\r\n}\r\n</script>\r\n\r\n"],"sourceRoot":"src\\components"}]}